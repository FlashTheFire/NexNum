{
  "name": "nexnum-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -k -p \" \" -n \"NEXT,WORKER\" -c \"blue.bold,magenta.bold\" \"next dev --turbo\" \"npm run worker\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "analyze": "ANALYZE=true next build",
    "analyze:win": "set ANALYZE=true && next build",
    "mock-api": "node src/lib/tests/mock-grizzly.mjs",
    "worker": "npx tsx -r dotenv/config src/worker-entry.ts",
    "verify:queue": "npx tsx -r dotenv/config scripts/verify-queue.ts",
    "prepare": "husky",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test": "vitest run",
    "test:load": "docker run --rm -i -v \"%cd%/scripts/load-testing:/src\" grafana/k6 run -e BASE_URL=http://host.docker.internal:3000 -e API_KEY=%API_KEY% /src/scenarios/public-api.js"
  },
  "dependencies": {
    "@hcaptcha/react-hcaptcha": "^2.0.2",
    "@hookform/resolvers": "^5.2.2",
    "@prisma/adapter-pg": "7.2.0",
    "@prisma/client": "7.2.0",
    "@radix-ui/react-accordion": "1.2.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "2.1.8",
    "@radix-ui/react-slot": "1.2.4",
    "@radix-ui/react-switch": "1.2.6",
    "@react-email/components": "^1.0.3",
    "@react-email/render": "^2.0.1",
    "@sentry/nextjs": "^10.32.1",
    "bcryptjs": "3.0.3",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "date-fns": "4.1.0",
    "dotenv": "^17.2.3",
    "framer-motion": "11.18.2",
    "ioredis": "^5.9.1",
    "jose": "6.1.3",
    "libphonenumber-js": "^1.12.35",
    "lucide-react": "^0.562.0",
    "meilisearch": "0.54.0",
    "next": "16.1.1",
    "next-auth": "^4.24.13",
    "next-intl": "^4.7.0",
    "next-sitemap": "^4.2.3",
    "next-themes": "^0.4.6",
    "opossum": "^9.0.0",
    "otplib": "^13.1.1",
    "pg": "8.16.3",
    "pg-boss": "^12.5.4",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "prisma": "7.2.0",
    "prom-client": "^15.1.3",
    "qrcode": "^1.5.4",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "^7.71.1",
    "recharts": "^3.6.0",
    "resend": "^6.6.0",
    "sonner": "1.7.4",
    "swagger-ui-react": "^5.31.0",
    "swr": "2.3.8",
    "tailwind-merge": "2.6.0",
    "tailwindcss-animate": "1.0.7",
    "web-push": "^3.6.7",
    "zod": "4.2.1",
    "zustand": "4.5.7"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^16.1.1",
    "@playwright/test": "^1.57.0",
    "@types/bcryptjs": "2.4.6",
    "@types/node": "20.19.27",
    "@types/opossum": "^8.1.9",
    "@types/pg": "8.16.0",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@types/web-push": "^3.6.4",
    "autoprefixer": "10.4.23",
    "concurrently": "^9.2.1",
    "eslint": "8.57.1",
    "eslint-config-next": "15.1.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "postcss": "8.5.6",
    "tailwindcss": "3.4.19",
    "typescript": "5.9.3",
    "vite-tsconfig-paths": "^6.0.3",
    "vitest": "^4.0.16"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix --max-warnings=0"
    ],
    "*.{js,mjs}": [
      "eslint --fix"
    ]
  }
}