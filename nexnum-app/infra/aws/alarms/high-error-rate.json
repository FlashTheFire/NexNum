{
    "AlarmName": "NexNum-HighErrorRate",
    "AlarmDescription": "Triggered when error rate exceeds 1%",
    "MetricName": "ErrorCount",
    "Namespace": "NexNum",
    "Statistic": "Sum",
    "Period": 300,
    "EvaluationPeriods": 2,
    "Threshold": 10,
    "ComparisonOperator": "GreaterThanThreshold",
    "TreatMissingData": "notBreaching",
    "ActionsEnabled": true,
    "AlarmActions": [
        "arn:aws:sns:${AWS_REGION}:${AWS_ACCOUNT_ID}:nexnum-alerts"
    ]
}