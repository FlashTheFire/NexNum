                    "action": "getNumberV2",
                    "service": "$service",
                    "country": "$country",
                    "maxPrice": "$maxPrice",
                    "providerIds": "$providerIds",
                    "exceptProviderIds": "$exceptProviderIds"
                }
            },
            "getStatus": {
                "method": "GET",
                "path": "https://smsbower.online/stubs/handler_api.php",
                "queryParams": {
                    "action": "getStatus",
                    "id": "$id"
                }
            },
            "setCancel": {
                "method": "GET",
                "path": "https://smsbower.online/stubs/handler_api.php",
                "queryParams": {
                    "action": "setStatus",
                    "status": "8",
                    "id": "$id"
                }
            },
            "getBalance": {
                "method": "GET",
                "path": "https://smsbower.online/stubs/handler_api.php",
                "queryParams": {
                    "action": "getBalance"
                }
            },
            "getPrices": {
                "method": "GET",
                "path": "https://smsbower.online/stubs/handler_api.php",
                "queryParams": {
                    "action": "getPricesV3",
                    "service": "$service",
                    "country": "$country"
                }
            }
        },
        "mappings": {
            "getCountriesList": {
                "type": "json_array",
                "fields": {
                    "id": "id",
                    "name_en": "eng",
                    "name_ru": "rus",
                    "name_cn": "chn"
                }
            },
            "getServicesList": {
                "type": "json_array",
                "rootPath": "services",
                "fields": {
                    "code": "code",
                    "name": "name"
                }
            },
            "getNumber": {
                "type": "json_object",
                "fields": {
                    "activation_id": "activationId",
                    "phone_number": "phoneNumber",
                    "cost": "activationCost",
                    "country_code": "countryCode",
                    "can_get_another_sms": "canGetAnotherSms",
                    "activation_time": "activationTime",
                    "operator": "activationOperator"
                },
                "transform": {
                    "cost": "number",
                    "can_get_another_sms": "boolean"
                }
            },
            "getStatus": {
                "type": "text_regex",
                "regex": "(?<status>STATUS_WAIT_CODE|STATUS_WAIT_RETRY|STATUS_CANCEL|STATUS_OK)(?::(?<code>.+))?",
                "fields": {
                    "status": "status",
                    "code": "code"
                },
                "conditionalFields": {
                    "STATUS_WAIT_CODE": {
                        "status": "PENDING"
                    },
                    "STATUS_WAIT_RETRY": {
                        "status": "PENDING",
                        "message": "Waiting for next sms"
                    },
                    "STATUS_CANCEL": {
                        "status": "CANCELLED"
                    },
                    "STATUS_OK": {
                        "status": "RECEIVED"
                    }
                }
            },
            "setCancel": {
                "type": "text_regex",
                "regex": "ACCESS_CANCEL",
                "fields": {
                    "status": "ACCESS_CANCEL"
                },
                "conditionalFields": {
                    "ACCESS_CANCEL": {
                        "status": "CANCELLED"
                    }
                }
            },
            "getBalance": {
                "type": "text_regex",
                "regex": "ACCESS_BALANCE:(?<balance>[0-9.]+)",
                "fields": {
                    "balance": "balance"
                },
                "transform": {
                    "balance": "number"
                }
            },
            "getPrices": {
                "type": "json_dictionary",
                "nestingLevels": {
                    "extractOperators": true
                },
                "fields": {
                    "cost": "price|cost|rate",
                    "count": "count|qty|stock",
                    "operator": "provider_id|$key",
                    "service": "$parentKey",
                    "country": "$grandParentKey"
                },
                "transform": {
                    "cost": "number",
                    "count": "number"
                }
            }
        }
    },
